    "xml.symbols.excluded": [],
    "xml.symbols.filters": [],
    "xml.symbols.maxItemsComputed": 5000,
    "xml.symbols.showReferencedGrammars": true,
    "xml.trace.server": "off",
    "xml.validation.disallowDocTypeDecl": false,
    "xml.validation.enabled": true,
    "xml.validation.filters": [
    {
        "pattern": "**.exsd",
        "noGrammar": "ignore",
        "schema": {
        "enabled": "never"
        }
    },
    {
        "pattern": "**{.project,.classpath,plugin.xml,feature.xml,category.xml,.target,.product}",
        "noGrammar": "ignore"
    }
    ],
    "xml.validation.namespaces.enabled": "always",
    "xml.validation.noGrammar": "hint",
    "xml.validation.resolveExternalEntities": false,
    "xml.validation.schema.enabled": "always",
    "xml.validation.xInclude.enabled": false,

    // ? yaml ?
    "yaml.completion": true,
    "yaml.customTags": [],
    "yaml.disableAdditionalProperties": false,
    "yaml.disableDefaultProperties": false,
    "yaml.extension.recommendations": true,
    "yaml.format.bracketSpacing": true,
    "yaml.format.enable": true,
    "yaml.format.printWidth": 100,
    "yaml.format.proseWrap": "preserve",
    "yaml.format.singleQuote": false,
    "yaml.hover": true,
    "yaml.keyOrdering": false,
    "yaml.maxItemsComputed": 5000,
    "yaml.schemas": {
        "https://raw.githubusercontent.com/compose-spec/compose-spec/master/schema/compose-spec.json": "/**/docker-compose*.yaml"
    },
    "yaml.schemaStore.enable": true,
    "yaml.schemaStore.url": "https://www.schemastore.org/api/json/catalog.json",
    "yaml.style.flowMapping": "allow",
    "yaml.style.flowSequence": "allow",
    "yaml.suggest.parentSkeletonSelectedFirst": false,
    "yaml.trace.server": "off",
    "yaml.validate": true,
    "yaml.yamlVersion": "1.2",

    // xml
    "[xml]": {
        "editor.rulers": [100],
        "editor.defaultFormatter": "redhat.vscode-xml",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.defaultFoldingRangeProvider": "redhat.vscode-xml",
    },

    // json
    "[json]": {
        "editor.rulers": [100],
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.defaultFoldingRangeProvider": "vscode.json-language-features",
    },

    // toml
    "[toml]": {
        "editor.rulers": [100],
        "editor.defaultFormatter": "tamasfe.even-better-toml",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.defaultFoldingRangeProvider": "tamasfe.even-better-toml",
    },

    // yaml
    "[yaml]": {
        "editor.rulers": [100],
        "editor.defaultFormatter": "redhat.vscode-yaml",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.defaultFoldingRangeProvider": "redhat.vscode-yaml",
        "editor.tabSize": 2,
        "editor.autoIndent": "keep",
    },

    // python
    "[python]": {
        "editor.rulers": [100],
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.formatOnSave": true,
        "editor.formatOnPaste": true,
        "editor.defaultFoldingRangeProvider": "ms-python.python",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
    },

    // cpp
    "[cpp]": {
        "editor.defaultFoldingRangeProvider": "zokugun.explicit-folding",
        "explicitFolding.rules": [
            {
                "beginRegex": "#if(?:n?def)?",
                "middleRegex": "#el(?:se|if)",
                "endRegex": "#endif"
            },
            {
                "begin": "/*",
                "end": "*/",
                "nested": false
            },
            {
                "begin": "//",
                "continuation": "\\",
                "nested": false
            }
        ]
    },

    // html
    "[html]": {
        "editor.defaultFoldingRangeProvider": "zokugun.explicit-folding",
        "explicitFolding.rules": [
            {
                "beginRegex": "<(?!area|base|br|col|embed|hr|img|input|link|menuitem|meta|param|source|track|wbr)([a-zA-Z0-9]+)[^>\/]*>",
                "endRegex": "<\\/\\1>"
            }
        ]
    },

    // ! FOR BLACK, MYPY ISORT, FLAKE8 settings should be overridden for an every project if needed.
    // ! In a workspace use config files in linters and formatters settings instead of separate options.
    // ! for flake8 use .flake8 file or override options (or flake-Pyproject).
    // ! for mypy, black, isort use pyproject.toml file (not override options).
    // ! for black also specify black 23.9.1 (if want 2 lines before import).
}
